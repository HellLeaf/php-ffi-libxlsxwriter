{
    "name": "kluevandrew/php-ffi-libxlsxwriter",
    "description": "todo",
    "type": "library",
    "require": {
        "php": ">=7.4",
        "ext-ffi": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "FFILibXlsxWriter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "cs": "phpcs --standard=.phpcs.xml",
        "fcs": "phpcbf",
        "t": "phpunit",
        "twc": "phpdbg -dffi.enable=1 -qrr ./vendor/bin/phpunit"
    }
}
